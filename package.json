{
  "name": "petfinder-site",
  "version": "1.0.0",
  "description": "template project for react",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "cross-env webpack-dev-server --env dev",
    "test": "cross-env webpack --env test && cross-env jest",
    "lint": "cross-env ./node_modules/.bin/eslint src/main/js/",
    "build": "cross-env webpack --progress --colors --env build",
    "build-release": "cross-env webpack -p --progress --colors --env release",
    "heroku-postbuild": "cross-env webpack -p --progress --colors --env release",
    "serve-prod": "NODE_ENV=production node server.js",
    "watch": "webpack --watch"
  },
  "jest": {
    "roots": [
      "<rootDir>/build/tests"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>"
    ],
    "verbose": true,
    "transform": {}
  },
